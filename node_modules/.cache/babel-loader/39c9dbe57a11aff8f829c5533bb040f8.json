AA;AAAA,KAAJ;;AAAA,oBAAb,cAAa;AAAA;AAAA;AAAA,KAAnB;;AAgBI,gBAAyC,MAAM,CAAC,IAAD,CAA/C;AAAA,MAAQ,QAAR,WAAQ,QAAR;AAAA,MAAiB,QAAjB,WAAiB,QAAjB;AAAA,MAA2B,UAA3B,WAA2B,UAA3B;;AACA,SAAM;AAAE,IAAA,QAAQ,EAAR,QAAF;AAAW,IAAA,QAAQ,EAAR,QAAX;AAAqB,IAAA,UAAU,EAAV,UAArB;AAAiC,IAAA,cAAc,EAAd;AAAjC,GAAN;AACN,CA3BA;;AA6BA,eAAe;AACb,EAAA,IAAI,EAAE,UADO;AAEb,EAAA,UAAU,EAAC;AAAE,IAAA,KAAI,EAAJ;AAAF,GAFE;AAGb,EAAA,KAHa,mBAGJ;AACP,QAAM,MAAK,GAAI,SAAS,EAAxB;;AACA,0BAA0C,cAAc,EAAxD;AAAA,QAAQ,IAAR,mBAAQ,IAAR;AAAA,QAAc,YAAd,mBAAc,YAAd;AAAA,QAA4B,SAA5B,mBAA4B,SAA5B;;AACA,0BAA0D,cAAc,CAAC,SAAD,CAAxE;AAAA,QAAQ,QAAR,mBAAQ,QAAR;AAAA,QAAiB,QAAjB,mBAAiB,QAAjB;AAAA,QAA2B,UAA3B,mBAA2B,UAA3B;AAAA,QAAuC,cAAvC,mBAAuC,cAAvC;;AACA,QAAM,gBAAe,GAAI,SAAnB,gBAAmB,GAAM;AAC7B,MAAA,MAAM,CAAC,IAAP,CAAY;AAAE,QAAA,IAAI,EAAE;AAAR,OAAZ,EAD6B,CACC;AAChC,KAFA;;AAGA,WAAO;AACL,MAAA,QAAQ,EAAR,QADK;AACI,MAAA,QAAQ,EAAR,QADJ;AACc,MAAA,UAAU,EAAV,UADd;AAC0B,MAAA,IAAI,EAAJ,IAD1B;AACgC,MAAA,YAAY,EAAZ,YADhC;AAEL,MAAA,cAAc,EAAd,cAFK;AAEU,MAAA,gBAAe,EAAf;AAFV,KAAP;AAGF;AAba,CAAf","sourcesContent":["<template>\n  <div class=\"wrapper\">\n    <img class=\"wrapper__img\" src=\"http://www.dell-lee.com/imgs/vue3/user.png\" alt />\n    <div class=\"wrapper__input\">\n      <input\n        type=\"text\"\n        class=\"wrapper__input__content\"\n        placeholder=\"请输入用户名 \"\n        v-model=\"username\"\n      />\n    </div>\n    <div class=\"wrapper__input\">\n      <input \n      type=\"password\"\n      class=\"wrapper__input__content\"\n      placeholder=\"请输入密码\"\n      v-model=\"password\"\n      />\n    </div>\n    <div class=\"wrapper__input\">\n      <input\n      type=\"password\"\n      class=\"wrapper__input__content\"\n      placeholder=\"确认密码\"\n      v-model=\"ensurement\"\n      />\n    </div>\n    <div class=\"wrapper__register-button\" @click=\"handleRegister\">注册</div>\n    <div class=\"wrapper__register-link\" @click=\"handleLoginClick\">已有账号去登陆</div>\n    <Toast v-if=\"show\" :message=\"toastMessage\" />\n  </div>\n</template>\n<script>\nimport { reactive , toRefs} from 'vue'\nimport { useRouter } from 'vue-router'\nimport { post } from '../../utils/request'\nimport Toast,{ useToastEffect } from '../../components/Toast.vue'\n\nconst useRegisterEffect = (showToast) => {\n  const router = useRouter()\n  const data = reactive({ \n    username: '',\n    password: '',\n    ensurement: ''\n  })\n\n  // 注册相关逻辑\n  const handleRegister = async () => {\n        try {\n          const result = await post('/api/user/register', {\n            username: data.username,\n            password: data.password\n          })\n          // result?.data?.errno === 0\n          if (result?.errno === 0) {\n            router.push({ name: 'Login' })\n          } else {\n            showToast('注册失败')\n          }\n        } catch (e) {\n          showToast('请求失败')\n        }\n      }\n      const { username,password, ensurement} = toRefs(data)\n      return{ username,password, ensurement, handleRegister}\n}\n\nexport default {\n  name: 'Register',\n  components:{ Toast },\n  setup () {\n    const router = useRouter()\n    const { show, toastMessage, showToast } = useToastEffect(); \n    const { username,password, ensurement, handleRegister } = handleRegister(showToast)\n    const handleLoginClick = () => {\n      router.push({ name: 'Login' })// 点击已有账号登录跳转到登录页面\n    }\n    return { \n      username,password, ensurement, show, toastMessage,\n      handleRegister,handleLoginClick }\n  }\n}\n</script>\n\n<style lang=\"scss\" scoped>\n@import \"../../style/viriables.scss\";\n.wrapper {\n  position: absolute;\n  top: 50%;\n  left: 0;\n  right: 0;\n  transform: translateY(-50%);\n  &__img {\n    display: block;\n    margin: 0 auto 0.4rem auto;\n    width: 0.66rem;\n    height: 0.66rem;\n  }\n  &__input {\n    box-sizing: border-box;\n    background: #f9f9f9;\n    border: 0.01rem solid rgba(0, 0, 0, 0.1);\n    // padding: .12rem 0 .12rem .16rem;\n    padding: 0 0.16rem;\n    // padding: .12rem .16rem .20rem;\n    border-radius: 0.06rem;\n    height: 0.48rem;\n    margin: 0rem 0.48rem 0.16rem 0.48rem;\n    &__content {\n      width: 100%;\n      // height: 100%;\n      border: none;\n      outline: none;\n      line-height: 0.48rem;\n      background: none;\n      font-size: 0.16rem;\n      color: $content-notice-fontcolor;\n      &::placeholder {\n        color: $content-notice-fontcolor;\n      }\n    }\n  }\n  &__register-button {\n    margin: 0.32rem 0.4rem 0.16rem 0.4rem;\n    line-height: 0.48rem;\n    background: #0091ff;\n    box-shadow: 0 4px 8px 0 rgba(0, 145, 255, 0.32);\n    border-radius: 4px;\n    color: #fff;\n    font-size: 0.16rem;\n    text-align: center;\n  }\n  &__register-link {\n    // margin: .16rem .12rem 0 1.18rem;\n    text-align: center;\n    font-size: 0.14rem;\n    color: $content-notice-fontcolor;\n  }\n}\n</style>\n"],"sourceRoot":""}]}