useBackRouterEffect()\r\n        getItemData()\r\n        return { item, handleBackClick }\r\n    }\r\n}\r\n</script>\r\n<style lang=\"scss\" scoped>\r\n@import '../../style/viriables.scss';\r\n    .wrapper{\r\n        padding: 0 .18rem;\r\n    }\r\n    .search{\r\n        height: .32rem;\r\n        display: flex;\r\n        margin: .14rem 0 .04rem 0;\r\n        line-height: .32rem;\r\n        border-radius: 0.16rem;\r\n        &__back{\r\n            width: .32rem;\r\n            font-size: .2rem;\r\n            color: #B6B6B6;\r\n        }\r\n        &__content{\r\n            display: flex;\r\n            flex: 1;\r\n            line-height: .32rem;\r\n            background-color: $search-bgColor;\r\n            border-radius: .16rem;\r\n            &__icon{\r\n                width: .44rem;\r\n                text-align: center;\r\n                font-size: .22rem;\r\n                color: $search-fontColor;\r\n            }\r\n            &__input{\r\n                display: block;\r\n                width: 100%;\r\n                padding-right: .2rem;\r\n                border:none;\r\n                outline: none;\r\n                height: .32rem;\r\n                font-size: .14rem;\r\n                // background:none;\r\n                border-radius: 0.16rem;\r\n                 background-color: $search-bgColor;\r\n                color:$content-fontcolor;\r\n                &::placeholder{\r\n                    color: $content-fontcolor;\r\n                }\r\n            }\r\n        }\r\n    }\r\n</style>"],"sourceRoot":""}]}