{"remainingRequest":"/Users/fudengwei/Desktop/jingdong/node_modules/babel-loader/lib/index.js!/Users/fudengwei/Desktop/jingdong/node_modules/eslint-loader/index.js??ref--14-0!/Users/fudengwei/Desktop/jingdong/src/router/index.js","dependencies":[{"path":"/Users/fudengwei/Desktop/jingdong/src/router/index.js","mtime":1638350049673},{"path":"/Users/fudengwei/Desktop/jingdong/babel.config.js","mtime":1638241175263},{"path":"/Users/fudengwei/Desktop/jingdong/node_modules/cache-loader/dist/cjs.js","mtime":1638184662125},{"path":"/Users/fudengwei/Desktop/jingdong/node_modules/babel-loader/lib/index.js","mtime":1638184662635},{"path":"/Users/fudengwei/Desktop/jingdong/node_modules/eslint-loader/index.js","mtime":1638184662203}],"contextDependencies":[],"result":[{"type":"Buffer","data":"base64:"},{"version":3,"sources":["/Users/fudengwei/Desktop/jingdong/src/router/index.js"],"names":["createRouter","createWebHashHistory","Home","Login","Register","routes","path","name","component","beforeEnter","to","from","next","isLogin","localStorage","router","history","beforeEach"],"mappings":";AAAA;AACA,SAASA,YAAT,EAAuBC,oBAAvB,QAAmD,YAAnD;AACA,OAAOC,IAAP,MAAiB,oBAAjB;AACA,OAAOC,KAAP,MAAkB,sBAAlB;AACA,OAAOC,QAAP,MAAqB,4BAArB;AAEA,IAAMC,MAAM,GAAG,CACb;AACEC,EAAAA,IAAI,EAAE,GADR;AAEEC,EAAAA,IAAI,EAAE,MAFR;AAGEC,EAAAA,SAAS,EAAEN;AAHb,CADa,EAMb;AACEI,EAAAA,IAAI,EAAE,WADR;AAEEC,EAAAA,IAAI,EAAE,UAFR;AAGEC,EAAAA,SAAS,EAAEJ;AAHb,CANa,EAWb;AACEE,EAAAA,IAAI,EAAE,QADR;AAEEC,EAAAA,IAAI,EAAE,OAFR;AAGEC,EAAAA,SAAS,EAAEL,KAHb;AAIE;AACA;AACAM,EAAAA,WANF,uBAMeC,EANf,EAMmBC,IANnB,EAMyBC,IANzB,EAM+B;AAC3B;AACA,QAAMC,OAAO,GAAGC,YAAY,CAACX,KAA7B;;AACA,QAAIU,OAAJ,EAAa;AACXD,MAAAA,IAAI,CAAC;AAAEL,QAAAA,IAAI,EAAE;AAAR,OAAD,CAAJ;AACD,KAFD,MAEO;AACLK,MAAAA,IAAI;AACL;AACF;AAdH,CAXa,CA2Bb;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AAlCa,CAAf;AAqCA,IAAMG,MAAM,GAAGf,YAAY,CAAC;AAC1BgB,EAAAA,OAAO,EAAEf,oBAAoB,EADH;AAE1BI,EAAAA,MAAM,EAANA;AAF0B,CAAD,CAA3B,C,CAIA;;AACAU,MAAM,CAACE,UAAP,CAAkB,UAACP,EAAD,EAAKC,IAAL,EAAWC,IAAX,EAAoB;AACpC,MAAMC,OAAO,GAAGC,YAAY,CAACX,KAA7B,CADoC,CAEpC;;AACA,MAAI,CAACU,OAAD,IAAaH,EAAE,CAACH,IAAH,KAAY,OAAZ,IAAuBG,EAAE,CAACH,IAAH,KAAY,UAApD,EAAiE;AAC/DK,IAAAA,IAAI,CAAC;AAAEL,MAAAA,IAAI,EAAE;AAAR,KAAD,CAAJ;AACD,GAFD,MAEO;AACLK,IAAAA,IAAI;AACL;AACF,CARD;AASA,eAAeG,MAAf","sourcesContent":["// import { is } from 'core-js/fn/object'\nimport { createRouter, createWebHashHistory } from 'vue-router'\nimport Home from '../views/home/Home'\nimport Login from '../views/login/Login'\nimport Register from '../views/register/Register'\n\nconst routes = [\n  {\n    path: '/',\n    name: 'Home',\n    component: Home\n  },\n  {\n    path: '/register',\n    name: 'Register',\n    component: Register\n  },\n  {\n    path: '/login',\n    name: 'Login',\n    component: Login,\n    // 进入前执行的函数,防止登录状态下还跳转到登录页面\n    // 访问login之前才会执行的函数\n    beforeEnter (to, from, next) {\n      // console.log(to, from)\n      const isLogin = localStorage.Login\n      if (isLogin) {\n        next({ name: 'Home' })\n      } else {\n        next()\n      }\n    }\n  }\n  // {\n  //   path: '/about',\n  //   name: 'About',\n  //   // route level code-splitting\n  //   // this generates a separate chunk (about.[hash].js) for this route\n  //   // which is lazy-loaded when the route is visited.\n  //   component: () => import(/* webpackChunkName: \"about\" */ '../views/About.vue')\n  // }\n]\n\nconst router = createRouter({\n  history: createWebHashHistory(),\n  routes\n})\n// 路由做什么切换时都会执行，路由守卫，用户没登录前自动跳转到登录界面\nrouter.beforeEach((to, from, next) => {\n  const isLogin = localStorage.Login\n  // 注册页面未登录的情况下不会跳转到登录页面\n  if (!isLogin && (to.name !== 'Login' && to.name !== 'Register')) {\n    next({ name: 'Login' })\n  } else {\n    next()\n  }\n})\nexport default router\n"]}]}