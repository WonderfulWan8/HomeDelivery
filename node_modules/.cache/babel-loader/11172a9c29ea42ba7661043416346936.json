AA,EAqBM,CApBF,mBAAA,CAKM,KALN,EAAA,UAAA,EAKM,CAJF,mBAAA,CAE0C,KAF1C,EAE0C;AAD1C,IAAA,KAAK,EAAC,4BACoC;AAAxC,IAAA,OAAK,EAAA,MAAA,CAAA,CAAA,CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA,UAAA,MAAA;AAAA,aAAE,MAAA,CAAA,eAAA,EAAF;AAAA,KAAA;AAAmC,GAF1C,EAE4B,GAF5B,CAIE,E,UAAA,CALN,CAoBE,EAdF,UAcE,CArBN,CAuDE,EAjCF,mBAAA,CA4BM,KA5BN,EAAA,UAAA,EA4BM,CA3BF,mBAAA,CAEM,KAFN,EAAA,UAAA,EAEM,gBAAA,CADA,MAAA,CAAA,QACA,CAFN,EACc;AAAA;AADd,GA2BE,EAxBF,mBAAA,CAuBM,KAvBN,EAAA,UAAA,EAuBM,E,kBArBF,mBAAA,CAoBW,SApBX,EAoBW,IApBX,EAoBW,WAAA,CAnBQ,MAAA,CAAA,WAmBR,EAnBmB,UAAnB,IAAmB,EAAf;;WACL,IAAI,CAAC;QAEuB,IAAI,CAAC,KAAL,GAAU,C,kBAA5C,mBAAA,CAeM,KAfN,EAAA,UAAA,EAeM,CAdF,mBAAA,CAA2D,KAA3D,EAA2D;AAArD,MAAA,GAAG,EAAE,IAAI,CAAC,MAA2C;AAAnC,MAAA,KAAK,EAAC,qBAA6B;AAAP,MAAA,GAAG,EAAC;AAAG,KAA3D,E,IAAA,E;;AAAA,M,WAAA,CAcE,EAbF,mBAAA,CAYM,KAZN,EAAA,WAAA,EAYM,CAXF,mBAAA,CAAoD,IAApD,EAAA,WAAA,EAAoD,gBAAA,CAAhB,IAAI,CAAC,IAAW,CAApD,EAA6C;AAAA;AAA7C,KAWE,EAVF,mBAAA,CASI,GATJ,EAAA,WAAA,EASI,CARA,mBAAA,CAGO,MAHP,EAAA,WAAA,EAGO,CAFH,WAEG,E,iBAF2C,MAC9C,gBAAA,CAAE,IAAI,CAAC,KAAP,CAD8C,GAChC,KADgC,GAC7B,gBAAA,CAAE,IAAI,CAAC,KAAP,C,EAAY;AAAA;KAC1B,CAHP,CAQA,EAJA,mBAAA,CAGO,MAHP,EAAA,WAAA,EAGO,CAFH,WAEG,E,iBAF2C,MAC9C,gBAAA,CAAA,CAAG,IAAI,CAAC,KAAL,GAAa,IAAI,CAAC,KAArB,EAA4B,OAA5B,CAAmC,CAAnC,CAAA,C,EAAmC;AAAA;KAChC,CAHP,CAIA,CATJ,CAUE,CAZN,CAaE,CAfN,C;;;GAgBO,CApBX,E;;AAAA,GAqBE,EAvBN,CAwBE,CA5BN,CAiCE,EAJF,mBAAA,CAGM,KAHN,EAAA,WAAA,EAGM,CAFF,mBAAA,CAAkE,KAAlE,EAAA,WAAA,EAAkE,C,WAAA,EAAnC,mBAAA,CAA6B,GAA7B,EAA6B,IAA7B,EAAG,MAAC,gBAAA,CAAE,MAAA,CAAA,WAAA,CAAY,KAAd,CAAJ,EAAuB;AAAA;AAAvB,GAAmC,CAAlE,CAEE,EADF,WACE,CAHN,CAIE,CAxDN,C","sourcesContent":["<template>\r\n    <div class=\"wrapper\">\r\n        <div class=\"top\"> \r\n            <div class=\"top__header\">\r\n                <div \r\n                class=\"iconfont top__header__back\"\r\n                 @click=\"handleBackClick()\">&#xe679;</div>\r\n                确认订单\r\n            </div>\r\n            <div class=\"top__receiver\">\r\n                <div class=\"top__receiver__title\">收货地址</div>\r\n                <div class=\"top__receiver__address\">北京理工大学国防科技园2号楼10层</div>\r\n                <div class=\"top__receiver__info\">\r\n                    <span class=\"top__receiver__info__name\">瑶妹（先生）</span>\r\n                    <span class=\"top__receiver__info__phoneNum\">18911024266</span>\r\n                </div>\r\n                <div\r\n                class=\"iconfont top__receiver__icon\"\r\n               \r\n                >\r\n                &#xe679;\r\n                </div>\r\n            </div>\r\n        </div>\r\n        <div class=\"products\">\r\n            <div class=\"products__title\">\r\n                {{shopName}}\r\n            </div> \r\n            <div class=\"products__list\">\r\n                \r\n                <template\r\n                    v-for=\"item in productList\"\r\n                    :key=\"item._id\"\r\n                >\r\n                    <div class=\"products__item\" v-if=\"item.count > 0\">\r\n                        <img :src=\"item.imgUrl\" class=\"products__item__img\" alt=\"\">\r\n                        <div class=\"products__item__detail\">\r\n                            <h4 class=\"products__item__title\">{{item.name}}</h4>\r\n                            <p class=\"products__item__price\">\r\n                                <span class=\"products__item__single\">\r\n                                    <span class=\"products__item__yen\">&yen;</span> \r\n                                    {{item.price}} × {{item.count}}\r\n                                </span>\r\n                                <span class=\"products__item__total\">\r\n                                    <span class=\"products__item__yen\">&yen;</span> \r\n                                    {{(item.price * item.count).toFixed(2)}}\r\n                                </span>\r\n                            </p>\r\n                        </div>\r\n                    </div>\r\n                </template>\r\n            </div>    \r\n        </div>\r\n        <div class=\"order\">\r\n            <div class=\"order__price\">实付金额 <b>¥{{caculations.price}}</b></div>\r\n            <div class=\"order__btn\">提交订单</div>\r\n        </div>\r\n    </div>    \r\n</template>\r\n\r\n<script>\r\nimport { useRoute, useRouter } from 'vue-router';\r\nimport { useCommomCartEffect } from '../../effects/cartEffects';\r\nexport default{\r\n    name: \"OrderConfirmation\",\r\n    setup(){\r\n        const route = useRoute();\r\n        const router = useRouter();\r\n        const shopId = route.params.id;\r\n        const { productList, shopName, caculations } = useCommomCartEffect(shopId);\r\n        const handleBackClick = ()=>{ router.back(); };\r\n        return { productList, shopName, caculations, handleBackClick }\r\n    }\r\n}\r\n</script>\r\n<style lang=\"scss\" scoped>\r\n@import '../../style/viriables.scss';\r\n@import '../../style/mixins.scss';   \r\n    .wrapper{\r\n        position: absolute;\r\n        top:0;\r\n        left: 0;\r\n        right: 0;\r\n        bottom: 0;\r\n        background-color: #eee;\r\n        overflow-y: scroll;\r\n    }\r\n    .top{\r\n        position: relative;\r\n        height: 1.96rem;\r\n        background-size:100% 1.59rem;//宽度100% 高度159px\r\n        background-image: linear-gradient(0deg, rgba(0,145,255,0.00) 4%, #0091FF 50%);\r\n        background-repeat: no-repeat;\r\n        position: relative;\r\n        &__header{\r\n            padding-top:.26rem;\r\n            line-height: .22rem;\r\n            font-size: .16rem;\r\n            color: white;\r\n            text-align: center;\r\n            &__back{\r\n                position: absolute;\r\n                left: .18rem;\r\n                font-size: .22rem;\r\n            }\r\n        }\r\n        &__receiver{\r\n            position: absolute;\r\n            left: .18rem;\r\n            right: .18rem;\r\n            bottom: 0;//有距离底部的距离，所以不用设置header和receiver之间的距离\r\n            height: 1.11rem;\r\n            background-color: white;    \r\n            color: #333;\r\n            border-radius: .04rem;\r\n            padding-left: .16rem;\r\n            &__title{\r\n                padding-top: .16rem;\r\n                font-size: .16rem;   \r\n                line-height: .22rem;\r\n            }\r\n            &__address{\r\n                padding-top: .14rem;\r\n                font-size: .14rem;\r\n                line-height: .20rem;\r\n            }\r\n            &__info{\r\n                padding-top: .06rem;\r\n                color: #666;\r\n                font-size: .12rem;\r\n                line-height: .18rem;\r\n                &__name{\r\n                    margin-right: .06rem;\r\n                }\r\n            }\r\n            &__icon{\r\n                transform: rotate(180deg);\r\n                position: absolute;\r\n                top:.48rem;\r\n                right: .16rem;   \r\n                font-size: .2rem;\r\n            }\r\n        }\r\n    }\r\n    .products{\r\n        margin:.16rem .18rem .55rem .18rem;//.55rem给底部实付金额留出位置\r\n        background-color: #fff;\r\n        &__title{\r\n           padding: 16px 0 0 16px;\r\n           font-size: 16px;\r\n           color: #333;\r\n           line-height: 22px;\r\n           font-weight: bold;\r\n        }\r\n        &__item{\r\n            overflow-y: scroll;//y轴滚动\r\n            flex: 1;\r\n                position: relative;\r\n                display: flex;\r\n                padding: .12rem 0;//下面有边框，用padding会有问题\r\n                margin: 0 .16rem;\r\n                border-bottom: .01rem solid $content-bgColor;\r\n                &__detail{\r\n                    overflow:hidden;\r\n                }\r\n                &__img{\r\n                    width: 0.46rem;\r\n                    height: 0.46rem;\r\n                    margin-right: .16rem;\r\n                }\r\n                &__title{\r\n                    line-height: .2rem;\r\n                    font-size: .14rem;\r\n                    color: $content-fontcolor;\r\n                    margin: 0;\r\n                    margin-bottom: 6px;\r\n                    @include ellipsis\r\n                    \r\n                }\r\n                &__price{\r\n                    display: flex;\r\n                    margin: 0;//重置p标签的margin\r\n                    line-height: .2rem;\r\n                    font-size: .14rem;\r\n                    color: $highlight-fontColor;\r\n                    \r\n                }\r\n                &__single{\r\n                    width: 160px;\r\n                }\r\n                &__total{\r\n                    color: #333;\r\n                    font-weight: bold;\r\n                }\r\n                &__yen{\r\n                    font-size: .12rem;\r\n                }\r\n        }\r\n    }\r\n    .order{\r\n        position: fixed;\r\n        left: 0px;\r\n        right: 0px;\r\n        bottom: 0px;\r\n        display: flex;\r\n        height: .49rem;\r\n        line-height: .49rem;\r\n        background-color: #fff;\r\n        &__price{\r\n            flex: 1;\r\n            color: #333;\r\n            font-size: .14rem;\r\n            // font-weight: bold;\r\n            margin-left: 24px;\r\n        }\r\n        &__btn{\r\n            color: white;\r\n            background-color: #4FB0F9;\r\n            width: .98rem;\r\n            font-size: .14rem;\r\n            text-align: center;\r\n        }\r\n    }\r\n</style>\r\n"],"sourceRoot":""}]}