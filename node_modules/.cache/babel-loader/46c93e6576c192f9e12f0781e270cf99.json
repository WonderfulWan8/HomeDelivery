;WACL,IAAI,CAAC;QAEuB,IAAI,CAAC,KAAL,GAAU,C,kBAA5C,mBAAA,CAeM,KAfN,EAAA,UAAA,EAeM,CAdF,mBAAA,CAA2D,KAA3D,EAA2D;AAArD,MAAA,GAAG,EAAE,IAAI,CAAC,MAA2C;AAAnC,MAAA,KAAK,EAAC,qBAA6B;AAAP,MAAA,GAAG,EAAC;AAAG,KAA3D,E,IAAA,E;;AAAA,M,UAAA,CAcE,EAbF,mBAAA,CAYM,KAZN,EAAA,UAAA,EAYM,CAXF,mBAAA,CAAoD,IAApD,EAAA,UAAA,EAAoD,gBAAA,CAAhB,IAAI,CAAC,IAAW,CAApD,EAA6C;AAAA;AAA7C,KAWE,EAVF,mBAAA,CASI,GATJ,EAAA,UAAA,EASI,CARA,mBAAA,CAGO,MAHP,EAAA,WAAA,EAGO,CAFH,WAEG,E,iBAF2C,MAC9C,gBAAA,CAAE,IAAI,CAAC,KAAP,CAD8C,GAChC,KADgC,GAC7B,gBAAA,CAAE,IAAI,CAAC,KAAP,C,EAAY;AAAA;KAC1B,CAHP,CAQA,EAJA,mBAAA,CAGO,MAHP,EAAA,WAAA,EAGO,CAFH,WAEG,E,iBAF2C,MAC9C,gBAAA,CAAA,CAAG,IAAI,CAAC,KAAL,GAAa,IAAI,CAAC,KAArB,EAA4B,OAA5B,CAAmC,CAAnC,CAAA,C,EAAmC;AAAA;KAChC,CAHP,CAIA,CATJ,CAUE,CAZN,CAaE,CAfN,C;;;GAgBO,CApBX,E;;AAAA,GAqBE,EAtBN,CAuBE,CAxBN,CAyBE,CA7BN,C","sourcesContent":["<template>\r\n    <div class=\"products\">\r\n        <div class=\"products__title\">\r\n            {{shopName}}\r\n        </div> \r\n        <div class=\"products__wrapper\">\r\n            <div class=\"products__list\">\r\n                <template\r\n                    v-for=\"item in productList\"\r\n                    :key=\"item._id\"\r\n                >\r\n                    <div class=\"products__item\" v-if=\"item.count > 0\">\r\n                        <img :src=\"item.imgUrl\" class=\"products__item__img\" alt=\"\">\r\n                        <div class=\"products__item__detail\">\r\n                            <h4 class=\"products__item__title\">{{item.name}}</h4>\r\n                            <p class=\"products__item__price\">\r\n                                <span class=\"products__item__single\">\r\n                                    <span class=\"products__item__yen\">&yen;</span> \r\n                                    {{item.price}} × {{item.count}}\r\n                                </span>\r\n                                <span class=\"products__item__total\">\r\n                                    <span class=\"products__item__yen\">&yen;</span> \r\n                                    {{(item.price * item.count).toFixed(2)}}\r\n                                </span>\r\n                            </p>\r\n                        </div>\r\n                    </div>\r\n                </template>\r\n            </div>\r\n        </div>  \r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport { useRoute } from 'vue-router';\r\nimport { useCommomCartEffect } from '../../effects/cartEffects';\r\nexport default{\r\n    name: \"ProductList\",\r\n    setup(){\r\n        const route = useRoute();\r\n        const shopId = route.params.id;\r\n        const { productList, shopName } = useCommomCartEffect(shopId);\r\n        return { productList, shopName }\r\n    }\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n\r\n@import '../../style/viriables.scss';\r\n@import '../../style/mixins.scss';\r\n    .products{\r\n        margin:.16rem .18rem .1rem .18rem;//.55rem给底部实付金额留出位置\r\n        background-color: $bgColor;\r\n        &__title{\r\n           padding: .16rem;\r\n           font-size: .16rem;\r\n           color: $content-fontcolor;\r\n           line-height: 22px;\r\n           font-weight: bold;\r\n        }\r\n        &__list{\r\n            background-color: white;\r\n            // padding-bottom: 0.16rem;\r\n        }\r\n        &__wrapper{\r\n            overflow-y: scroll;\r\n            margin: 0 .18rem;\r\n            position: absolute;\r\n            left: 0;\r\n            right: 0;\r\n            top: 2.6rem;\r\n            bottom: .6rem;\r\n        }\r\n        &__item{\r\n            flex: 1;\r\n            position: relative;\r\n            display: flex;\r\n            padding: 0 .16rem .16rem .16rem;\r\n            &__detail{\r\n                overflow:hidden;\r\n            }\r\n            &__img{\r\n                width: 0.46rem;\r\n                height: 0.46rem;\r\n                margin-right: .16rem;\r\n            }\r\n            &__title{\r\n                line-height: .2rem;\r\n                font-size: .14rem;\r\n                color: $content-fontcolor;\r\n                margin: 0;\r\n                margin-bottom: 6px;\r\n                @include ellipsis\r\n                \r\n            }\r\n            &__price{\r\n                display: flex;\r\n                margin: 0;//重置p标签的margin\r\n                line-height: .2rem;\r\n                font-size: .14rem;\r\n                color: $highlight-fontColor;\r\n                \r\n            }\r\n            &__single{\r\n                width: 155px;\r\n            }\r\n            &__total{\r\n                color: $dark-fontColor;\r\n                font-weight: bold;\r\n            }\r\n            &__yen{\r\n                font-size: .12rem;\r\n            }\r\n        }\r\n    }\r\n\r\n</style>"],"sourceRoot":""}]}