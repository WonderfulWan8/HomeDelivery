EAAE,OAFL;AAGDC,EAAAA,SAAS,EAAE;AAAA,WAAM;AAAO;AAAgC,0BAAvC,CAAN;AAAA,GAHV;AAIDC,EAAAA,WAJC,uBAIWC,EAJX,EAIeC,IAJf,EAIqBC,IAJrB,EAI2B;AAC1B,yBAAoBC,YAApB;AAAA,QAAQC,OAAR,kBAAQA,OAAR;AACAA,IAAAA,OAAO,GAAGF,IAAI,CAAC;AAAEL,MAAAA,IAAI,EAAE;AAAR,KAAD,CAAP,GAA2BK,IAAI,EAAtC;AACD;AAPA,CAvBU,CAgCb;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AAvCa,CAAf;AA0CA,IAAMG,MAAM,GAAGb,YAAY,CAAC;AAC1Bc,EAAAA,OAAO,EAAEb,oBAAoB,EADH;AAE1BE,EAAAA,MAAM,EAANA;AAF0B,CAAD,CAA3B,C,CAIA;;AACAU,MAAM,CAACE,UAAP,CAAkB,UAACP,EAAD,EAAKC,IAAL,EAAWC,IAAX,EAAoB;AACpC,uBAAoBC,YAApB;AAAA,MAAQC,OAAR,kBAAQA,OAAR;AACA,MAAQP,IAAR,GAAiBG,EAAjB,CAAQH,IAAR,CAFoC,CAGpC;;AACA,MAAMW,iBAAiB,GAAIX,IAAI,KAAK,OAAT,IAAoBA,IAAI,KAAK,UAAxD;AACCO,EAAAA,OAAO,IAAII,iBAAZ,GAAiCN,IAAI,EAArC,GAA0CA,IAAI,CAAC;AAAEL,IAAAA,IAAI,EAAE;AAAR,GAAD,CAA9C;AACD,CAND;AAOA,eAAeQ,MAAf","sourcesContent":["// import { is } from 'core-js/fn/object'\nimport { createRouter, createWebHashHistory } from 'vue-router'\nimport Login from '../views/login/Login'\n\nconst routes = [{\n    path: '/',\n    name: 'Home',\n    // 动态路由，只有访问时才会加载\n    component: () => import(/* webpackChunkName: \"home\" */ '../views/home/Home')\n  }, {\n    path: '/cartList',\n    name: 'CartList',\n    // 动态路由，只有访问时才会加载\n    component: () => import(/* webpackChunkName: \"cartList\" */ '../views/home/Home')\n  }, {\n    path: '/shop/:id',\n    name: 'Shop',\n    // 动态路由，只有访问时才会加载\n    component: () => import(/* webpackChunkName: \"shop\" */ '../views/shop/Shop')\n  }, {\n    path: '/register',\n    name: 'Register',\n    component: () => import(/* webpackChunkName: \"register\" */ '../views/register/Register'),\n    beforeEnter(to, from, next) {\n      const { isLogin } = localStorage;\n      isLogin ? next({ name: 'Home'}):  next();\n    }\n  }, {\n    path: '/login',\n    name: 'Login',\n    component: () => import(/* webpackChunkName: \"login\" */ '../views/login/Login'),\n    beforeEnter(to, from, next) {\n      const { isLogin } = localStorage;\n      isLogin ? next({ name: 'Home'}):  next();\n    }\n  },\n  // {\n  //   path: '/about',\n  //   name: 'About',\n  //   // route level code-splitting\n  //   // this generates a separate chunk (about.[hash].js) for this route\n  //   // which is lazy-loaded when the route is visited.\n  //   component: () => import(/* webpackChunkName: \"about\" */ '../views/About.vue')\n  // }\n]\n\nconst router = createRouter({\n  history: createWebHashHistory(),\n  routes\n})\n// 路由做什么切换时都会执行，路由守卫，用户没登录前自动跳转到登录界面\nrouter.beforeEach((to, from ,next) => {\n  const { isLogin } = localStorage;\n  const { name } = to;\n  // 注册页面未登录的情况下不会跳转到登录页面\n  const isLoginOrRegister = (name === \"Login\" || name === \"Register\");\n  (isLogin || isLoginOrRegister) ? next() : next({ name: 'Login'});\n})\nexport default router"]}]}